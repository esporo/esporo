{
  "actions" : {
    "build" : {
      "enabled" : true,
      "script" : "#!\/bin\/sh\nbundle update"
    },
    "clean" : {
      "enabled" : true,
      "script" : "#!\/bin\/sh\nrm -rf .jekyll-metadata .jekyll-cache"
    },
    "run" : {
      "enabled" : true,
      "script" : "#!\/bin\/sh\nbundle exec jekyll serve --drafts"
    }
  },
  "openLogOnRun" : "start"
}
